<!DOCTYPE html>
<html>
<head>
  <title>Blog</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if user_signed_in? %>
	Welcome <%= current_user.email %> | <%= link_to "Account Settings", edit_user_registration_path %> | <%= link_to "Logout", destroy_user_session_path, method: :delete %>
<%else%>
	<%= link_to "Login", new_user_session_path %> | <%= link_to "Register", new_user_registration_path %>
<%end%> </br></br>

<%= link_to "Categories", categories_path %> | <%= link_to "Articles", articles_path %>

<%= alert %>
<%= notice %>

<%= yield %>

</body>
</html>
